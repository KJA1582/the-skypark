                    <TextBlock Name="appVersion" Text="000000" Foreground="#88000000" VerticalAlignment="Center" Margin="0,0,10,0" FontSize="12" PreviewMouseLeftButtonDown="AppVersion_MouseLeftButtonUp"/>
